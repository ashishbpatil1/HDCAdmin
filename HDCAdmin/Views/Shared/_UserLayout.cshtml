<!DOCTYPE html>

<html data-ng-app="app">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Deshmukh Constructions</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/font-awesome/css")
    @Styles.Render("~/Content/usercss")
    @Styles.Render("~/Content/usercss/colors")
    <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/jquery.bootstrapvalidator/0.5.2/css/bootstrapValidator.min.css" />

</head>
<body>
    <div>
        <!-- Styleswitcher
        ================================================== -->
        <div class="colors-switcher">
            <a id="show-panel" class="hide-panel" title="Reach Us"><i class="faa-shake animated fa fa-phone"></i></a>
            <ul class="colors-list">
                <li style="color:green;"><b> +91-8888436777 </b></li>
                <li style="color:green;"><b> +91-9028112344 </b></li>
            </ul>

        </div>
        <!-- Styleswitcher End
        ================================================== -->
        <!-- Start Loader -->
        <div id="loader">
            <div class="spinner">
                <div class="dot1"></div>
                <div class="dot2"></div>
            </div>
        </div>
        <!-- End Loader -->
        <!-- Start Navigation Section -->
        <div class="navigation" style="margin-bottom:55px;">

            <div class="navbar navbar-default navbar-fixed-top">
                <div class="navbar-header">
                    <!-- Stat Toggle Nav Link For Mobiles -->
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <i class="fa fa-bars"></i>
                    </button>

                </div>
                <div class="navbar-collapse collapse">
                    <div class="container">
                        <!-- Start Navigation List -->
                        <ul class="nav navbar-nav">
                            <li>
                                <a href="@Url.Action("Index", "User")">Home</a>
                            </li>
                            <li>
                                <a href="@Url.Action("AboutUs", "User")">About Us</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Services", "User")">Services</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Projects", "User")">Projects</a>
                                <ul class="dropdown">
                                    <li>
                                        <a href="@Url.Action("Projects", "User")">Dollotrao Park</a>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a href="@Url.Action("ContactUs", "User")">Contact Us</a>
                            </li>
                        </ul>
                        <!-- End Navigation List -->
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                @Html.ActionLink("Log in", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink" })
                            </li>
                        </ul>
                    </div>
                </div>
            </div>

        </div>
        <!-- End Navigation Section -->

        <div class="container">
            <!-- Start Header Section -->
            <div class="header-section">
                <div class="row">
                    <div class="col-md-5 col-sm-5">
                        <div class="logo-img">
                            <a href="#"><img src="~/Content/images/Logo2.png" class="img-responsive" alt=""></a>
                        </div>
                    </div>
                    <div class="col-md-7 col-sm-7">
                        <div class="top-info">
                            <ul class="top-social">
                                <li><a href="#" class="facebook"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="#" class="twitter"><i class="fa fa-twitter"></i></a></li>
                                <!--<li><a href="#" class="linkedin"><i class="fa fa-linkedin"></i></a></li>
                                <li><a href="#" class="flickr"><i class="fa fa-flickr"></i></a></li>-->
                                <li><a href="#" class="google-plus"><i class="fa fa-google-plus"></i></a></li>
                            </ul>
                            <div class="top-phone clearfix"><i class="fa fa-phone"></i>+91-8888436777</div>
                            <div class="top-phone clearfix"><i class="fa fa-phone"></i>+91-9028112344</div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Header Section -->
            @RenderBody()

            <!-- Start Footer Section -->
            <div class="row">
                <div class="col-md-12">
                    <div class="footer-section">

                        <div class="row">

                            <div class="col-md-3 col-sm-6">
                                <div class="footer-address">
                                    <h3>Address</h3>
                                    <address>
                                        <strong>Deshmukh Constructions</strong><br>
                                        2B, Saneguruji Housing Society,<br>
                                        Near Swami Samarth Kendra,<br>
                                        Amalner - 425 401, Dist. Jalgaon
                                    </address>

                                    <address>
                                        <strong>Harshal Deshmukh :</strong> (+91) 8888436777<br>
                                        <strong>Harish Deshmukh :</strong> (+91) 9028112344<br>
                                        <strong>E-mail :</strong> deshmukhconstructions15@gmail.com
                                    </address>
                                </div>
                            </div>

                            <div class="col-md-3 col-sm-6">
                                <div class="footer-gallery">
                                    <h3>Photo Gallery</h3>
                                    <ul>
                                        <li>
                                            <a href="/Content/images/HDrowhouses glass railing1.jpg" data-lightbox="project-1">
                                                <img src="~/Content/images/HDrowhouses glass railing1.jpg" class="img-responsive" alt="">
                                            </a>
                                        </li>
                                        <li>
                                            <a href="/Content/images/HD SiteMap.jpg" data-lightbox="project-4">
                                                <img src="~/Content/images/HD SiteMap.jpg" class="img-responsive" alt="">
                                            </a>
                                        </li>
                                        <li>
                                            <a href="/Content/images/HDrowhouse1.jpg" data-lightbox="project-2">
                                                <img src="~/Content/images/HDrowhouse1.jpg" class="img-responsive" alt="">
                                            </a>
                                        </li>
                                        <li>
                                            <a href="/Content/images/HDrowhouse2.jpg" data-lightbox="project-3">
                                                <img src="~/Content/images/HDrowhouse2.jpg" class="img-responsive" alt="">
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div class="col-md-3 col-sm-6">
                                <div class="footer-social">
                                    <h3>Get In Touch</h3>
                                    <ul>
                                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                        <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                                    </ul>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>
            <!-- End Footer Section -->
            <!-- Start Copyright Section -->
            <div class="row">
                <div class="col-md-12">
                    <div class="copyright-section">
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <div class="copy">Deshmukh Constructions &copy; @DateTime.Now.Year - All Rights Reserved</div>
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <!--<div class="design">Design & Developed by <a href="http://graygrids.com">GrayGrids</a></div>-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Copyright Section -->

            <a href="#" class="back-to-top"><i class="fa fa-angle-up"></i></a>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery.bootstrapvalidator/0.5.2/js/bootstrapValidator.min.js"></script>
    @Scripts.Render("~/bundles/user")

    @*@Scripts.Render("~/bundles/angularjs")
        @Scripts.Render("~/bundles/uibootstrap")
        @Scripts.Render("~/bundles/appjs")*@

    <script src="http://maps.google.com/maps/api/js?sensor=false"></script>

    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        $(function () {
            $('a[href="' + this.location.pathname + '"]').addClass('active');

            if (this.location.pathname == "/User/ContactUs") {
                function init_map1() {
                    var myLocation = new google.maps.LatLng(21.039054, 75.057968);
                    var mapOptions = {
                        center: myLocation,
                        zoom: 16
                    };
                    var marker = new google.maps.Marker({
                        position: myLocation,
                        title: "Deshmukh Constructions Office"
                    });
                    var map = new google.maps.Map(document.getElementById("map1"),
                        mapOptions);
                    marker.setMap(map);
                }
                init_map1();
            }
        });
    </script>
</body>
</html>
